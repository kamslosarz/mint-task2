{% extends 'base.html.twig' %}

{% block title %}Sing up!{% endblock %}

{% block body %}
    <form method="post">
        {% if error is defined %}
            <div class="alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
        {% endif %}

        {{ form_start(form) }}
        {{ form_end(form) }}
    </form>
    <a href="{{ path('app_login') }}">Sing in</a>
{% endblock %}
